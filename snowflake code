CAST(TFM.BIDPOOLID AS NUMBER) AS BIDPOOLID
,TFM.SOURCE_DATA_OBJECT:data:BidPoolName::string AS BIDPOOLNAME
,TO_TIMESTAMP_NTZ((CASE WHEN lower(TFM.SOURCE_DATA_OBJECT:data:BidPoolEffectiveDate::string) = 'null' THEN NULL
   ELSE TFM.SOURCE_DATA_OBJECT:data:BidPoolEffectiveDate::string END)) AS BIDPOOLEFFECTIVEDATE
,TO_TIMESTAMP_NTZ((CASE WHEN lower(TFM.SOURCE_DATA_OBJECT:data:BidPoolEndDate::string) = 'null' THEN NULL
   ELSE TFM.SOURCE_DATA_OBJECT:data:BidPoolEndDate::string END)) AS BIDPOOLENDDATE
,TO_NUMBER((CASE WHEN lower(TFM.SOURCE_DATA_OBJECT:data:BidPoolBlockOutEndDayCount::string) = 'null' THEN NULL
  ELSE TFM.SOURCE_DATA_OBJECT:data:BidPoolBlockOutEndDayCount::string END),38,0) AS BIDPOOLBLOCKOUTENDDAYCOUNT
,TFM.SOURCE_DATA_OBJECT:data:BidPoolBlockOutEndWeekdayName::string AS BIDPOOLBLOCKOUTENDWEEKDAYNAME
,TFM.SOURCE_DATA_OBJECT:data:BlockOutExempt::string AS BLOCKOUTEXEMPT
,TFM.SOURCE_DATA_OBJECT:data:CreateUserId::string AS CREATEUSERID
,TO_TIMESTAMP_NTZ((CASE WHEN lower(TFM.SOURCE_DATA_OBJECT:data:CreateDatetime::string) = 'null' THEN NULL
   ELSE TFM.SOURCE_DATA_OBJECT:data:CreateDatetime::string END)) AS CREATEDATETIME
,TFM.SOURCE_DATA_OBJECT:data:UpdateUserId::string AS UPDATEUSERID
,TO_TIMESTAMP_NTZ((CASE WHEN lower(TFM.SOURCE_DATA_OBJECT:data:UpdateDatetime::string) = 'null' THEN NULL
   ELSE TFM.SOURCE_DATA_OBJECT:data:UpdateDatetime::string END)) AS UPDATEDATETIME
,TO_NUMBER((CASE WHEN lower(TFM.SOURCE_DATA_OBJECT:data:VacationBidTypeId::string) = 'null' THEN NULL
  ELSE TFM.SOURCE_DATA_OBJECT:data:VacationBidTypeId::string END),38,0) AS VACATIONBIDTYPEID
,TO_NUMBER((CASE WHEN lower(TFM.SOURCE_DATA_OBJECT:data:TimeOffCalendarId::string) = 'null' THEN NULL
  ELSE TFM.SOURCE_DATA_OBJECT:data:TimeOffCalendarId::string END),38,0) AS TIMEOFFCALENDARID
, CURRENT_TIMESTAMP(9) AS INSERT_DTS
, CURRENT_TIMESTAMP(9) UPDT_DTS
,CAST('UTC' AS VARCHAR) AS TM_ZN
,CAST('CMA' AS VARCHAR) AS SRC_SYS_NM
,CAST(CURRENT_SESSION() AS VARCHAR) AS PROC_ID
,CAST('cma_bidpool_syndicate_load' AS VARCHAR) AS PROC_NM
,TFM.INSTNC_ST_NM AS INSTNC_ST_NM
, TFM.ETL_CHECKSUM AS ETL_CHECKSUM
, TFM.SOURCE_FILE_NAME AS SOURCE_FILE_NAME
, TFM.SOURCE_FILE_ROW_NUMBER AS SOURCE_FILE_ROW_NUMBER
